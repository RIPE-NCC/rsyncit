#uid = nobody
#gid = nobody
use chroot = no
max connections = 150
timeout = 180
dont compress = *
log file = /var/log/rsyncd/rsyncd.log
pid file = /var/run/rsyncd.pid
lock file = /var/run/rsyncd.lock
[ta]
comment = Trust Anchor Repository
path = /data/published/rpki.ripe.net/ta/
read only = true
list = true

[repository]
comment = Members RPKI Repository
path = /data/published/rpki.ripe.net/repository/
read only = true
list = true